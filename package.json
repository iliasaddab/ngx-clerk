{
  "name": "ngx-clerk-iliad",
  "version": "0.2.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && cp ./README.md ./dist/ngx-clerk-iliad/README.md",
    "watch": "ng build --watch --configuration development",
    "version:minor": "cd projects/ngx-clerk-iliad && npm version minor",
